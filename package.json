{
  "name": "@lightdotso/api",
  "version": "0.0.0",
  "private": true,
  "homepage": "https://github.com/LightDotSo/LightDotSo",
  "bugs": {
    "url": "https://github.com/LightDotSo/LightDotSo/issues"
  },
  "repository": "https://github.com/LightDotSo/LightDotSo",
  "license": "UNLICENSED",
  "author": "Light, Inc.",
  "main": "dist/worker.js",
  "scripts": {
    "build": "pnpm run clean && pnpm run prisma:cp && pnpm run prisma:generate && pnpm run next:build || pnpm run nest:build",
    "build:cache": "webpack -c webpack.cache.js",
    "build:image": "webpack -c webpack.image.js",
    "clean": "rimraf -rf ./dist",
    "cypress:ci": "cypress run --record",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "dev": "next dev",
    "dev:cache": "miniflare --watch -c wrangler.cache.toml",
    "dev:image": "miniflare --watch -c wrangler.image.toml",
    "env:local": "doppler secrets download --no-file --format env > .env.local",
    "env:prisma": "doppler secrets download --no-file --format env > prisma/.env",
    "env:vercel": "vercel pull --env .env.local",
    "fix": "cd ../.. && pnpm run fix",
    "prisma:cp": "cp ../../packages/prisma/schema.prisma prisma/schema.prisma",
    "prisma:db:push": "prisma db push",
    "prisma:generate": "prisma generate",
    "prisma:studio": "prisma studio",
    "restore": "git restore . --recurse-submodules",
    "nest:build": "nest build -p tsconfig.nest.json",
    "nest:dev": "nest start -p tsconfig.nest.json --watch",
    "nest:start": "nest start -p tsconfig.nest.json",
    "next:build": "next build",
    "next:dev": "next dev",
    "next:start": "next start",
    "start": "pnpm run next:start"
  },
  "dependencies": {
    "@bull-board/api": "^4.0.0",
    "@bull-board/express": "^4.0.0",
    "@ethersproject/abstract-provider": "^5.6.1",
    "@ethersproject/address": "^5.6.1",
    "@ethersproject/providers": "^5.6.8",
    "@lightdotso/const": "workspace:*",
    "@lightdotso/queries": "workspace:*",
    "@lightdotso/services": "workspace:*",
    "@lightdotso/types": "workspace:*",
    "@logtail/node": "^0.1.12",
    "@logtail/winston": "^0.1.12",
    "@nestjs/axios": "^0.0.8",
    "@nestjs/bull": "^0.5.5",
    "@nestjs/common": "^8.4.7",
    "@nestjs/config": "^2.1.0",
    "@nestjs/core": "^8.4.7",
    "@nestjs/passport": "^8.2.2",
    "@nestjs/platform-express": "^8.4.7",
    "@nestjs/schedule": "^2.0.1",
    "@nestjs/terminus": "^8.0.8",
    "@ntegral/nestjs-sentry": "^3.0.7",
    "@prisma/client": "^3.15.2",
    "@sentry/integrations": "^7.2.0",
    "@sentry/nextjs": "^7.2.0",
    "@sentry/node": "^7.2.0",
    "bull": "^4.8.4",
    "cross-fetch": "^3.1.5",
    "ethers": "^5.6.9",
    "ethers-multicall": "^0.2.3",
    "graphql": "^16.5.0",
    "jose": "^4.8.1",
    "nanoid": "^4.0.0",
    "nest-winston": "^1.6.2",
    "next": "^12.1.6",
    "next-compose-plugins": "^2.2.1",
    "passport": "^0.6.0",
    "passport-http": "^0.3.0",
    "prisma": "^3.15.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.5.5",
    "winston": "^3.7.2",
    "worktop": "0.8.0-next.14",
    "zod": "^3.17.3",
    "zod-prisma": "^0.5.4"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^3.13.0",
    "@cloudflare/wrangler": "^1.19.12",
    "@cypress/react": "^6.0.0",
    "@nestjs/cli": "^8.2.6",
    "@nestjs/testing": "^8.4.7",
    "@testing-library/cypress": "^8.0.3",
    "@types/bull": "^3.15.8",
    "@types/passport": "^1.0.9",
    "@types/passport-http": "^0.3.9",
    "@types/react": "^18.0.14",
    "cypress": "^10.2.0",
    "miniflare": "^2.5.1",
    "supertest": "^6.2.3",
    "ts-loader": "^9.3.1",
    "tsconfig-paths-webpack-plugin": "^3.5.2",
    "webpack": "^5.73.0",
    "webpack-cli": "^4.10.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  }
}
